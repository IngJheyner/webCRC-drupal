{
    "name": "metadrop/drupal-boilerplate",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "^1.7",
        "drupal-composer/drupal-security-advisories": "8.x-dev",
        "drupal/admin_audit_trail": "^1.0@alpha",
        "drupal/admin_toolbar": "^2.4",
        "drupal/advagg": "^4.1",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/big_pipe_sessionless": "^2.0",
        "drupal/blazy": "^2.4",
        "drupal/config_split": "^1.7",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-recommended": "^9.1",
        "drupal/dropzonejs": "^2.5",
        "drupal/entity_browser": "^2.6",
        "drupal/environment_indicator": "^4.0",
        "drupal/eu_cookie_compliance": "^1.13",
        "drupal/facets": "^1.8",
        "drupal/field_group": "^3.2",
        "drupal/gin": "^3.0@alpha",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/google_analytics": "^3.1",
        "drupal/honeypot": "^2.0",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/metatag": "^1.14",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/private_files_download_permission": "^3.0",
        "drupal/quicklink": "^1.3",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/radix": "^4.10",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.5",
        "drupal/remove_http_headers": "^1.0",
        "drupal/scheduler": "^1.4",
        "drupal/search_api": "^1.20",
        "drupal/simple_sitemap": "^3.8",
        "drupal/slick": "^2.3",
        "drupal/slick_views": "^2.4",
        "drupal/stage_file_proxy": "^1.1",
        "drupal/styleguide": "^2.0@beta",
        "drupal/superfish": "^1.4",
        "drupal/svg_image": "^1.15",
        "drupal/twig_tweak": "^3.1",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/webform": "^6.0@alpha",
        "drupal/xmlsitemap": "^1.0",
        "drush/drush": "^10.3"
    },
    "require-dev": {
        "metadrop/drupal-dev": "^0.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "discard-changes": true
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "patches": {
            "drupal/redis": {
                "Default lifetime for permanent TTL": "https://www.drupal.org/files/issues/2020-09-23/redis-default_permanent_ttl-2944938.patch"
            }
        },
        "composer-exit-on-patch-failure": true,
        "enable-patching": true
    }
}
